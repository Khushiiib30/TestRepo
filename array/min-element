#include <stdio.h>
int main(void)
{
    int a[10],i,n,min;
    printf("enter the size of array: ");
    scanf("%d",&n);
    printf("enter the elements: ");
    for(i=0;i<n;i++)
        scanf("%d",&a[i]);
    min=a[0];
    for(i=0;i<n;i++)
        if(a[i]<min)
            min=a[i];
    printf("min element is %d ", min);
}
