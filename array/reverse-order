#include <stdio.h>
void main()
{
    int a[100],i,n;
    printf("enter the size of array: ");
    scanf("%d",&n);
    printf("enter the elements: ");
    for(i=0;i<n;i++)
        scanf("%d",&a[i]);
    printf("the given elements are: ");
    for(i=0;i<n;i++)
        printf("\n%d ",a[i]);
    printf("\nthe given elements in reverse are: ");
    for(i=n-1;i>=0;i--)
        printf("\n%d ",a[i]);
    
}
