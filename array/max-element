#include <stdio.h>
int main(void)
{
    int a[10],i,n,max;
    printf("enter the size of array: ");
    scanf("%d",&n);
    printf("enter the elements: ");
    for(i=0;i<n;i++)
        scanf("%d",&a[i]);
    max=a[0];
    for(i=0;i<n;i++)
        if(a[i]>max)
            max=a[i];
    printf("max element is %d ", max);
}
